{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*\\.(?:js|mjs|css|ico|png|jpg|jpeg|webp|svg|woff2?|ttf|map|json|html))",
      "destination": "/dist/fine-line-tattoo/browser/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/fine-line-tattoo/server/server.mjs"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(?:js|mjs|css|ico|png|jpg|jpeg|webp|svg|woff2?|ttf|map|json|html))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
